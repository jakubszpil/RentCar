<Window x:Class="RentCar.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RentCar"
        mc:Ignorable="d"
        Title="Wypożyczalnia samochodów - Panel zarządzający"
        MinWidth="1136"
        Height="720" 
        Width="1280"
        HorizontalContentAlignment="Stretch"
        HorizontalAlignment="Stretch"
       >
    <DockPanel LastChildFill="True">
        <Frame x:Name="Navbar" NavigationUIVisibility="Hidden" HorizontalContentAlignment="Stretch"
        HorizontalAlignment="Stretch" DockPanel.Dock="Top" />
        <Frame x:Name="RouterView" NavigationUIVisibility="Hidden" HorizontalContentAlignment="Stretch"
        HorizontalAlignment="Stretch" />
    </DockPanel>
</Window>
